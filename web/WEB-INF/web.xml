<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>china.gangzhong.Hello</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/lookHello</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>computer</servlet-name>
        <servlet-class>china.gangzhong.Computer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>computer</servlet-name>
        <url-pattern>/getNumber</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>show</servlet-name>
        <servlet-class>china.gangzhong.ShowLetter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>show</servlet-name>
        <url-pattern>/helpMeShow</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>computerPI</servlet-name>
        <servlet-class>china.gangzhong.ComputerPI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>computerPI</servlet-name>
        <url-pattern>/computerPI</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ok</servlet-name>
        <servlet-class>china.gangzhong.Show</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ok</servlet-name>
        <url-pattern>/show</url-pattern>
    </servlet-mapping>
</web-app>